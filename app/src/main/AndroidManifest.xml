<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mansi.busezon">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SELL_BUY" />
        <activity
            android:name=".SELL"
            android:label="@string/category_SELL">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".BUY"
            android:label="@string/category_BUY">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".profile_page"
            android:label="@string/category_profile">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".SELL_BUY" />
    </activity>

    <activity android:name=".Add_product"></activity>
    </application>

</manifest>